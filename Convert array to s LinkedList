#include <iostream>
#include<bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node* next;
    Node(int data1, Node* next1){
        data=data1;
        next=next1;
    }
};

Node* convertArraytoLL(vector<int> &arr){
   Node* head=new Node(arr[0],NULL);
   Node* mover=head;
   for(int i=1;i<arr.size();i++){
       Node* temp=new Node(arr[i], NULL);
       mover->next=temp;
       mover=mover->next;
   }
   return head;
}
int main() {
    // Write C++ code here
    std::cout << "Try programiz.pro";
    vector<int> arr={3,5,2,1,7};
    Node* head=convertArraytoLL(arr);
    cout<<endl;
    cout<<head->data;
    return 0;
}
